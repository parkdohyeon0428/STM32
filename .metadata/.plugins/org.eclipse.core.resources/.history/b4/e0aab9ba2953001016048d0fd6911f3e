#ifndef AP_INC_MODEL_FLOOR_H_
#define AP_INC_MODEL_FLOOR_H_

#include "stm32f4xx_hal.h"



typedef struct {
	uint8_t isRunning;     // 1: 동작 중, 0: 정지
	uint16_t speed;        // 현재 속도
	uint16_t motorFreq;    // 모터 주파수 (필요시)
	uint16_t buzzerFreq;   // 버저 주파수 (필요시)
} watch_t;

#endif /* AP_INC_MODEL_FLOOR_H_ */
