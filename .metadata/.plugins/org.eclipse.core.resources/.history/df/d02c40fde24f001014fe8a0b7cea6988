#include "ap_main.h"

Button_Handler_t hbtnLeft;
Button_Handler_t hbtnRight;
Button_Handler_t hbtnOnOff;

void HAL_TIM_PeriodElapsedHalfCpltCallback(TIM_HandleTypeDef *htim)
{
	if(htim->Instance == TIM2) {
	   FND_DispData();

	}
} // interrupt 때 실행할 코드 넣기
  // 여기에 넣으면 동일한 이름의 weak 붙은 함수는 실행 x

int ap_main()
{
	FND_WriteData(1234);
	HAL_TIM_Base_Start_IT(&htim2);
	while(1)
   {

   }

   return 0;
}

void ap_init()
{
   Button_Init(&hbtnLeft, GPIOB, GPIO_PIN_5);
   Button_Init(&hbtnRight, GPIOB, GPIO_PIN_3);
   Button_Init(&hbtnOnOff, GPIOA, GPIO_PIN_10);
}
