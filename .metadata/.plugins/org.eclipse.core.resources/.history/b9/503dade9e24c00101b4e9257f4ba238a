
#include <stdint.h>

#include "ap_main.h"


int main(void)
{
	ap_init();
	fnd_Init();

	while(1)
	{
		ap_main();
		fnd_Write(0x03);

		GPIO_WritePin(FND_COMM[0].GPIOx, FND_COMM[0].pinNum, PIN_RESET); // 첫 자리 ON
		GPIO_WritePin(FND_COMM[1].GPIOx, FND_COMM[1].pinNum, PIN_SET);
		GPIO_WritePin(FND_COMM[2].GPIOx, FND_COMM[2].pinNum, PIN_SET);
		GPIO_WritePin(FND_COMM[3].GPIOx, FND_COMM[3].pinNum, PIN_SET);
	}


	return 0;
}

