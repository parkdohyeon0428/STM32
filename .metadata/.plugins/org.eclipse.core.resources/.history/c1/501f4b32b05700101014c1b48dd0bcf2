#include "Model.h"



eStopWatchState_t StopWatchState = S_STOPWATCH_STOP;

void Model_SetStopWatchState(eStopWatchState_t state)
{
	StopWatchState = state;
}

eStopWatchState_t Model_GetStopWatchState()
{
	return StopWatchState;
}

